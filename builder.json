{
  "productName": "图小小",
  "appId": "com.tuxiaoxiao.picmin",
  "directories": {
    "output": "release",
    "buildResources": "buildResources"
  },
  "asar": false,
  "files": ["dist/**", "bin/**"],
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      },
      {
        "target": "portable",
        "arch": [
          "x64"
        ]
      }
    ]
  },
  "mac": {
    "category": "public.app-category.graphics-design",
    "target": {
      "target": "dmg",
      "arch": ["x64", "arm64"]
    }
  }
}
