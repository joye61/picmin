{
  "name": "txx-compress-app",
  "description": "An image compress app build with electron",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./scripts/main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./scripts/renderer.prod.ts",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "package": "ts-node ./scripts/clean.ts && npm run build && electron-builder build --publish never --config ./builder.json",
    "postinstall": "electron-builder install-app-deps",
    "start": "npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron -r ts-node/register/transpile-only ./src/main/main.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./scripts/renderer.dev.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joye61/txx-compress-app.git"
  },
  "author": {
    "name": "joye",
    "email": "89065495@qq.com"
  },
