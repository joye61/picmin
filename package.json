{
  "name": "picmin",
  "private": true,
  "version": "0.0.1",
  "author": "Joye",
  "description": "A powerful image compression tool",
  "main": "./dist/main/main.js",
  "scripts": {
    "start": "concurrently --timings npm:start:renderer npm:start:main",
    "start:main": "tsc -p ./main/tsconfig.json && wait-on tcp:4000 && electron --trace-warnings .",
    "start:renderer": "vite dev .",
    "build:main": "tsc -p ./main/tsconfig.json",
    "build:renderer": "tsc -p ./renderer/tsconfig.json && vite build",
    "build": "npm run build:main && npm run build:renderer",
    "package": "npm run build && electron-builder -c ./builder.json",
    "postinstall": "electron-builder install-app-deps",
    "test": "cross-env TS_JEST_DISABLE_VER_CHECKER=true jest --config ./jest.config.ts"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "filesize": "^8.0.7",
    "fs-extra": "^10.0.1",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.3.0",
    "normalize.css": "^8.0.1",
    "probe-image-size": "^7.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "readdirp": "^3.6.0",
    "sharp": "^0.30.3",
    "svgo": "^2.8.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/probe-image-size": "^7.0.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/sharp": "^0.30.0",
    "@types/svgo": "^2.6.2",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.2.0",
    "electron-builder": "^22.14.13",
    "electron-rebuild": "^3.2.7",
    "jest": "^27.5.1",
    "sass": "^1.49.9",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "wait-on": "^6.0.1"
  }
}
